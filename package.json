{
  "name": "candy-red",
  "version": "5.1.0",
  "description": "CANDY RED Device Gateway Service powered by Node-RED",
  "main": "./dist/index.js",
  "scripts": {
    "start": "NODE_PALETTE_ENABLED=true node ./dist/index.js",
    "build": "gulp build",
    "clean": "gulp clean",
    "test": "gulp test",
    "preinstall": "./install.sh test",
    "postinstall": "./install.sh",
    "preuninstall": "./uninstall.sh",
    "prepublish": "if [ -d ./src ] ; then gulp test && gulp clean && gulp build; fi",
    "freeze": "npm prune && npm shrinkwrap --only=prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CANDY-LINE/candy-red.git"
  },
  "keywords": [
